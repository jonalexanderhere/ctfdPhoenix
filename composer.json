{
  "name": "ctfd/vercel-php",
  "description": "CTFd deployment untuk Vercel dengan PHP",
  "type": "project",
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-curl": "*",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*",
    "ext-pdo_pgsql": "*",
    "guzzlehttp/guzzle": "^7.5",
    "vlucas/phpdotenv": "^5.5"
  },
  "autoload": {
    "psr-4": {
      "CTFd\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}

